<table>
  <tr>
    <th>Title</th>
    <th>Name</th>
    <th>Address</th>
    <th>Product</th>
    <th>Quantity</th>
    <th>Total price</th>
  </tr>
  <% @orders.each do |order| %>
    <tr>
      <td><%= order.id %> </td>
      <td><%= order.name %> </td>
      <td><%= order.address %> </td>
    </tr>
  <% end %> 
</table> 